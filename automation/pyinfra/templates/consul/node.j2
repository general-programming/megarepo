data_dir = "/var/lib/consul"
log_level = "INFO"
server = false
ui = false
enable_local_script_checks = true
datacenter = "{{ consul_datacenter }}"
retry_join = {{ consul_servers }}
{% raw %}
bind_addr = "{{ GetPrivateInterfaces | include \"network\" \"10.101.0.0/24\" | attr \"address\" }}"
{% endraw %}